{
  "name": "ReduxDemoApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npm run androidList && react-native run-android",
    "androidNPX": "npm run androidList && npx react-native run-android",
    "androidRelease": "npm run androidList && react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "iosNPX": "npx react-native run-ios",
    "updateAsset": "react-native-asset",
    "clearCache": "npm start -- --reset-cache",
    "cleanXcode": "cd ios && xcodebuild clean && cd ../",
    "cleanGradle": "cd android && ./gradlew clean && cd ../",
    "clearBoth": "npm run cleanGradle && npm run cleanXcode && sleep 5s",
    "iPhone7": "react-native run-ios --simulator='iPhone 7'",
    "iPhone8": "react-native run-ios --simulator='iPhone 8'",
    "iPhoneSE2": "react-native run-ios --simulator='iPhone SE (2nd generation)'",
    "iPhone12Mini": "react-native run-ios --simulator='iPhone 12 mini'",
    "iPhone8Plus": "react-native run-ios --simulator='iPhone 8 Plus'",
    "iPhone11ProMax": "react-native run-ios --simulator='iPhone 11 Pro Max'",
    "iPadPro2": "react-native run-ios --simulator='iPad Pro (12.9-inch) (2nd generation)'",
    "iPadPro3": "react-native run-ios --simulator='iPad Pro (12.9-inch) (3rd generation)'",
    "start": "react-native start",
    "common": "npm run nodeInstall && npm run podInstall && npm run clearBoth",
    "nodeInstall": "npm run removeNodes && npm install",
    "installRelease": "cd android && ./gradlew installRelease && cd ../",
    "assembleRelease": "cd android && ./gradlew assembleRelease && cd ../",
    "bundleRelease": "cd android && ./gradlew bundleRelease && cd ../",
    "makeAPK": "npm run cleanGradle && npm run assembleRelease",
    "makeAAB": "npm run cleanGradle && npm run bundleRelease",
    "podInstall": "cd ios && pod install && cd ../",
    "podUninstall": "cd ios && pod deintegrate && cd ../",
    "podSetup": "npm run removePodLock && npm run podUninstall && npm run podInstall",
    "removeNodes": "rm -rf node_modules && rm -rf ./ios/Pods && sleep 10s",
    "removePodLock": "rm ./ios/Podfile.lock && sleep 2s",
    "removePkgLock": "rm package-lock.json && sleep 2s",
    "removeOld": "npm run removeNodes && npm run removePodLock && npm run removePkgLock",
    "androidList": "adb devices",
    "iPhoneList": "xcrun simctl list devices",
    "test": "jest",
    "lint": "eslint .",
    "formatProjectSrc": "eslint --fix src",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@react-native-community/async-storage": "1.12.1",
    "axios": "0.21.1",
    "lodash": "4.17.21",
    "native-base": "2.15.2",
    "patch-package": "^6.4.7",
    "prop-types": "15.7.2",
    "react": "17.0.1",
    "react-native": "0.64.0",
    "react-native-busy-indicator": "1.1.0",
    "react-native-device-info": "8.0.8",
    "react-native-exit-app": "1.1.0",
    "react-native-gesture-handler": "1.10.3",
    "react-native-iphone-x-helper": "1.3.1",
    "react-native-reanimated": "1.13.2",
    "react-native-root-toast": "3.2.1",
    "react-native-router-flux": "4.2.0",
    "react-native-screens": "2.17.1",
    "react-native-splash-screen": "3.2.0",
    "react-redux": "7.2.0",
    "redux": "4.0.5",
    "redux-form": "8.3.5",
    "redux-form-validators": "3.3.2",
    "redux-persist": "6.0.0",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "rest-redux": "0.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.13.14",
    "@babel/runtime": "7.13.10",
    "@react-native-community/eslint-config": "2.0.0",
    "babel-jest": "26.6.3",
    "eslint": "7.14.0",
    "jest": "26.6.3",
    "metro-react-native-babel-preset": "0.64.0",
    "react-test-renderer": "17.0.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
